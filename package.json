{
  "name": "archive-tool",
  "version": "1.0.1",
  "description": "archive files to zip/tar",
  "keywords": [
    "archiver",
    "archiver zip",
    "archiver tar",
    "node project archiver"
  ],
  "dependencies": {
    "archiver": "^2.1.1",
    "chalk": "^2.3.2",
    "date-fns": "^1.29.0",
    "fs-extra": "^5.0.0",
    "lodash.merge": "^4.6.1",
    "nodeinstall": "^0.1.3",
    "shelljs": "^0.8.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "codecov": "^3.0.0",
    "conventional-changelog-cli": "^1.3.5",
    "cross-env": "^5.0.5",
